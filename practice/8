<script src="https://code.jquery.com/jquery-1.12.4.min.js"></script
<script>
  $(function(){
    $("button").on("click", function(){
      var newText = $("h1").html();
      $("#text").html(newText);
    });
  });
</script>

<body>
  <h1>html() 메소드</h1>
  <p>버튼을 누르세요</p>
  <button>버튼</button>
  <p id="text"></p>
</body>
