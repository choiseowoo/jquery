<script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
	<script>
    $(function(){
      $("button").on("click", function(){
        $("#list")
          .find("li")
          .eq(1).html("두번쨰 아이템")
          .end()
          .eq(2).html("세번째 아이템");
      });
    });
  </script>
  
  <body>
    <h1>메소드 체이닝</h1>
    <ul id ="list">
      <li>첫번쨰 아이템</li>
      <li>두번쨰 아이템</li>
      <li>세번쨰 아이템</li>
      </ul>
      <button>버트</button>
  </body>
